{
  "name": "react-animation-wrapper",
  "version": "0.0.2",
  "description": "A set of react components wrapper to add animation, like animate.css",
  "main": "./lib/index.js",
  "scripts": {
    "start": "parcel ./docs/index.html -p 1234",
    "clean:docs": "rm -rf ./docs/dist",
    "clean:lib": "rm -rf ./lib",
    "build:docs": "npm run clean:docs && parcel build ./docs/index.html --out-dir ./docs/dist",
    "build:lib": "npm run clean:lib && parcel build ./src/index.js --out-dir ./lib",
    "precommit": "lint-staged",
    "deploy": "gh-pages -d /docs/dist",
    "prepublish": "npm run build:lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "!(node_modules)/}**/*.{js,jsx}": ["prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyan33/react-animation-wrapper.git"
  },
  "keywords": ["react", "wrapper", "component", "animation", "animate", "css"],
  "author": "Chang Yan <thomasyim94@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyan33/react-animation-wrapper/issues"
  },
  "homepage": "https://github.com/cyan33/react-animation-wrapper#readme",
  "dependencies": {
    "glamor": "^2.20.40",
    "highlight.js": "^9.12.0",
    "parcel-bundler": "^1.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-plugin-auto-import": "^0.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "1.9.2"
  }
}
